{
  "name": "tech-inspect-bff",
  "version": "3.0.3",
  "description": "Servico utilizado pelo tech-inspect-dash",
  "author": "Vitor Barbosa de Lima Santos",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org vitorblsantos --project tech-inspect ./dist && sentry-cli sourcemaps upload --org vitorblsantos --project tech-inspect ./dist"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.13.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.2.8",
    "@nestjs/microservices": "^10.2.8",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/typeorm": "^10.0.0",
    "@sentry/cli": "^2.37.0",
    "@sentry/nestjs": "^8.34.0",
    "@sentry/profiling-node": "^8.34.0",
    "@types/multer": "^1.4.12",
    "axios": "^1.7.7",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^2.0.0",
    "crypto": "^1.0.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "firebase": "^10.14.0",
    "firebase-admin": "^12.6.0",
    "form-data": "^4.0.0",
    "install": "^0.13.0",
    "multer": "^1.4.5-lts.1",
    "npm": "^10.9.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.7.36",
    "@swc/helpers": "^0.5.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^15.0.2",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "regenerator-runtime": "^0.14.1",
    "source-map-support": "^0.5.21",
    "standard": "^17.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "imports": {
    "@": "./src/"
  },
  "keywords": [
    "API",
    "NestJS"
  ],
  "jest": {
    "moduleFileExtensions": [
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
